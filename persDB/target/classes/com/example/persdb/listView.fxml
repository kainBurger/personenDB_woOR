<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.persdb.ListController">
    <TableView fx:id="tvItems">
    </TableView>
    <HBox spacing="10.0">
        <Button fx:id="btRefresh" onAction="#btRefreshClicked" text="refresh" />
        <Button fx:id="btInsert" onAction="#btInsertClicked" text="insert" />
      <VBox.margin>
         <Insets bottom="5.0" right="5.0" top="5.0" />
      </VBox.margin>
      <padding>
         <Insets bottom="5.0" right="5.0" top="5.0" />
      </padding>
    </HBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
